    "npm": "6.7.0"
  },
  "private": true,
  "proxy": "http://localhost:5678",
  "dependencies": {
    "bcrypt": "^3.0.1",
    "body-parser": "^1.18.3",
    "concurrently": "^4.0.1",
    "express": "^4.16.3",
